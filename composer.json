{
  "name" : "los/loslog",
  "description" : "LosLog provides some log utility",
  "require" : {
    "php" : ">=5.6.0",
    "psr/log" : "~1.0",
    "zendframework/zend-stratigility" : "~1.1",
    "zendframework/zend-diactoros" : "~1.1",
    "zendframework/zend-log" : "~2.6",
    "rollbar/rollbar" : "~0"
  },
  "require-dev" : {
    "satooshi/php-coveralls" : "dev-master",
    "mikey179/vfsStream" : "~1.6",
    "phpunit/phpunit" : "~5.0"
  },
  "license" : "BSD-3-Clause",
  "keywords" : [ "los", "log", "module", "zf2", "expressive" ],
  "autoload-dev" : {
    "psr-4" : {
      "LosMiddlewareTest\\LosLog\\" : "tests/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-develop" : "2.0-dev"
    }
  },
  "autoload" : {
    "psr-4" : {
      "LosMiddleware\\LosLog\\" : "src/"
    }
  },
  "suggest" : {
    "rollbar/rollbar" : "Needed if you want to use Rollbar logger"
  },
  "type" : "library",
  "authors" : [ {
    "name" : "Leandro Silva",
    "email" : "leandro@leandrosilva.info",
    "homepage" : "http://leandrosilva.info"
  } ],
  "homepage" : "http://github.com/Lansoweb/LosLog"
}

