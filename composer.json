{
  "name" : "los/loslog",
  "description" : "LosLog provides some log utility",
  "require" : {
    "php" : "^7.1",
    "psr/log" : "^1.0",
    "zendframework/zend-stratigility" : "^3.0",
    "zendframework/zend-diactoros" : "^1.7",
    "zendframework/zend-log" : "^2.9",
    "psr/container": "^1.0"
  },
  "require-dev" : {
    "satooshi/php-coveralls" : "^2.0",
    "mikey179/vfsStream" : "^1.6",
    "phpunit/phpunit" : "^7.0",
    "rollbar/rollbar": "^1.4",
    "zendframework/zend-coding-standard": "^1.0",
    "squizlabs/php_codesniffer": "^2.7",
    "phpstan/phpstan": "^0.9.2"
  },
  "license" : "MIT",
  "keywords" : [ "los", "log", "module", "zf2", "zf3", "expressive" ],
  "autoload-dev" : {
    "psr-4" : {
      "LosMiddlewareTest\\LosLog\\" : "tests/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-develop" : "2.0-dev"
    }
  },
  "autoload" : {
    "psr-4" : {
      "LosMiddleware\\LosLog\\" : "src/"
    }
  },
  "suggest" : {
    "rollbar/rollbar" : "Needed if you want to use Rollbar logger"
  },
  "type" : "library",
  "authors" : [ {
    "name" : "Leandro Silva",
    "email" : "leandro@leandrosilva.info",
    "homepage" : "http://leandrosilva.info"
  } ],
  "homepage" : "http://github.com/Lansoweb/LosLog",
  "scripts": {
    "check": [
      "@cs-check",
      "@phpstan"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "phpstan": "phpstan analyse -l 4 -c phpstan.neon src"
  }
}
