{
    "name": "los/loslog",
    "description": "LosLog provides some log utility your ZF2 application",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "los",
        "log",
        "module",
        "zf2"
    ],
    "homepage": "http://github.com/Lansoweb/LosLog",
    "authors": [
        {
            "name": "Leandro Silva",
            "email": "lansoweb@hotmail.com",
            "homepage": "https://github.com/Lansoweb/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*"
    },
    "suggest": {
        "doctrine/doctrine-module": "doctrine-module if you want to use the SqlLogger",
        "doctrine/doctrine-orm-module": "doctrine-orm-module if you want to use the EntityLogger"
    },
    "minimum-stability": "alpha",
    "autoload": {
        "psr-0": {
            "LosLog\\": "src/",
            "LosLogTest\\": "tests/"
        }
    }
}
